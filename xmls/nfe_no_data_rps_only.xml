<Rps>
    <InfRps Id="{{order_id_val}}">
        <IdentificacaoRps>
            <Numero>{{order_id_val}}</Numero>
            <Serie>1</Serie>
            <Tipo>1</Tipo>
        </IdentificacaoRps>
        <DataEmissao>{{order_created_at_val}}</DataEmissao>
        <NaturezaOperacao>1</NaturezaOperacao>
        <RegimeEspecialTributacao>6</RegimeEspecialTributacao>
        <OptanteSimplesNacional>1</OptanteSimplesNacional>
        <IncentivadorCultural>2</IncentivadorCultural>
        <Status>1</Status>
        <Servico>
            <Valores>
                <ValorServicos>{{order_total_price_val}}</ValorServicos>
                <IssRetido>2</IssRetido>
                <ValorIss>{{order_iss_price_val}}</ValorIss>
                <DescontoCondicionado>0.00</DescontoCondicionado>
                <Aliquota>5</Aliquota>
            </Valores>
            <ItemListaServico>1.05</ItemListaServico>
            <CodigoTributacaoMunicipio>10300700</CodigoTributacaoMunicipio>
            <Discriminacao>{{order_commentary_val}}</Discriminacao>
            <CodigoMunicipio>4314902</CodigoMunicipio>
        </Servico>
        <Prestador>
            <Cnpj>{{company_cnpj_val}}</Cnpj>
            <InscricaoMunicipal>{{company_inscription_val}}</InscricaoMunicipal>
        </Prestador>
        <Tomador>
            <IdentificacaoTomador>
                <CpfCnpj>
                    {{user_id_tag_val}}
                </CpfCnpj>
            </IdentificacaoTomador>
            <RazaoSocial>{{user_full_name_val}}</RazaoSocial>
            <Endereco>
                <Endereco>{{user_street_val}}</Endereco>
                <Numero>{{user_street_number_val}}</Numero>
                <Complemento>{{user_complement_val}}</Complemento>
                <Bairro>{{user_neighborhood_val}}</Bairro>
                <CodigoMunicipio>{{user_city_code_val}}</CodigoMunicipio>
                <Uf>{{user_state_val}}</Uf>
                <Cep>{{user_zip_code_val}}</Cep>
            </Endereco>
        </Tomador>
    </InfRps>
</Rps>